<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="my_paper_format" model="report.paperformat">
        <field name="name">My Paper</field>
        <field name="format">A5</field>
    </record>

    <record id="report_print_loan" model="ir.actions.report">
        <field name="name">Loan Details</field>
        <field name="model">applicant.apply</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">loansystem.loan_deatail</field>
        <field name="report_file">loansystem.loan_deatail</field>
        <field name="print_report_name">'Loan Detail - %s' % (object.employee_id)</field>
        <field name="binding_model_id" ref="model_applicant_apply"/>
    </record>
</odoo>