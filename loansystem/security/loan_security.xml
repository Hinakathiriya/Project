<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="0">

    <!-- Category -->
    <record model="ir.module.category" id="module_category_loansystem">
        <field name="name">Loansystem</field>
        <field name="description">Helps you handle your Estate Property.</field>
        <field name="sequence">10</field>
    </record>

    <record model="ir.module.category" id="module_category_loansystem2">
        <field name="name">Loansystem</field>
        <field name="description">Helps you handle your Estate Property</field>
        <field name="sequence">10</field>
        <field name="parent_id" ref="module_category_loansystem"/>
    </record>

    <!-- access Right -->
    <record id="group_loan_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_loansystem2"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_loan_administrator" model="res.groups">
        <field name="name">Administrator</field>
        <field name="category_id" ref="module_category_loansystem2"/>
        <field name="implied_ids" eval="[(4, ref('group_loan_user'))]"/>
    </record>



</data>
</odoo>